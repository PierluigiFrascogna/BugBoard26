{
    "name": "BugBoard Service",
    "dockerComposeFile": [
        "../../compose.base.yaml",
        "../../compose.localdev.yaml"
    ],
    "service": "bugboard",
    "runServices": [
        "bugboard"
    ],
    "workspaceFolder": "/BugBoard26/backend/bugboard",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "vmware.vscode-spring-boot",
                "vscjava.vscode-lombok",
                "vscjava.vscode-spring-boot-dashboard",
                "vscjava.vscode-spring-initializr"
            ]
        }
    },
    "remoteUser": "vscode"
}